{
  "name": "node-auth-jwt",
  "version": "1.0.0",
  "description": "A simple application with some REST APIs to perform authentication with JWT",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "db:init": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migration:g": "sequelize migration:generate --name",
    "db:seed": "sequelize db:seed:all",
    "db:seed:g": "sequelize seed:generate --name"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusk196/node-auth-jwt.git"
  },
  "keywords": [
    "node",
    "jwt",
    "auth"
  ],
  "author": "Sayantan Roy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dusk196/node-auth-jwt/issues"
  },
  "homepage": "https://github.com/dusk196/node-auth-jwt#readme",
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
